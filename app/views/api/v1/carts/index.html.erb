<p id="notice"><%= notice %></p>

<h1>Api V1 Carts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_carts.each do |api_v1_cart| %>
      <tr>
        <td><%= link_to 'Show', api_v1_cart %></td>
        <td><%= link_to 'Edit', edit_api_v1_cart_path(api_v1_cart) %></td>
        <td><%= link_to 'Destroy', api_v1_cart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Cart', new_api_v1_cart_path %>
