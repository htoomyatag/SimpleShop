<p id="notice"><%= notice %></p>

<h1>Api V1 Countries</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Country code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_countries.each do |api_v1_country| %>
      <tr>
        <td><%= api_v1_country.title %></td>
        <td><%= api_v1_country.country_code %></td>
        <td><%= link_to 'Show', api_v1_country %></td>
        <td><%= link_to 'Edit', edit_api_v1_country_path(api_v1_country) %></td>
        <td><%= link_to 'Destroy', api_v1_country, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Country', new_api_v1_country_path %>
