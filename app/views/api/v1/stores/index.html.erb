<p id="notice"><%= notice %></p>

<h1>Api V1 Stores</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Region</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_stores.each do |api_v1_store| %>
      <tr>
        <td><%= api_v1_store.title %></td>
        <td><%= api_v1_store.region_id %></td>
        <td><%= link_to 'Show', api_v1_store %></td>
        <td><%= link_to 'Edit', edit_api_v1_store_path(api_v1_store) %></td>
        <td><%= link_to 'Destroy', api_v1_store, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Store', new_api_v1_store_path %>
