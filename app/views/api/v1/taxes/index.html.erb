<p id="notice"><%= notice %></p>

<h1>Api V1 Taxes</h1>

<table>
  <thead>
    <tr>
      <th>Tax category</th>
      <th>Tax rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_taxes.each do |api_v1_tax| %>
      <tr>
        <td><%= api_v1_tax.tax_category %></td>
        <td><%= api_v1_tax.tax_rate %></td>
        <td><%= link_to 'Show', api_v1_tax %></td>
        <td><%= link_to 'Edit', edit_api_v1_tax_path(api_v1_tax) %></td>
        <td><%= link_to 'Destroy', api_v1_tax, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Tax', new_api_v1_tax_path %>
